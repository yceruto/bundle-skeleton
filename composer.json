{
    "name": "yceruto/bundle-skeleton",
    "type": "library",
    "license": "MIT",
    "description": "A minimal Symfony bundle template recommended to create new bundles",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "yceruto/bundle-flex": "^0.1",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "require-dev": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yceruto/bundle-flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Acme\\AcmeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\AcmeBundle\\Tests\\": "tests/"
        }
    }
}
